{
  "name": "@rockpack/codestyle",
  "version": "0.9.9",
  "description": "The best practice for ESLint and TSLint",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint \"**/*.js\"",
    "test": "node tests.js",
    "test:watch": "node tests.js --watch",
    "production": "npm run lint && npm run test && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexSergey/rock.git"
  },
  "author": "Aleksandrov Sergey <gooddev.sergey@gmail.com> (https://github.com/AlexSergey/rock)",
  "license": "MIT",
  "homepage": "https://www.rock-book.io/",
  "bugs": {
    "url": "https://github.com/AlexSergey/rock/issues"
  },
  "keywords": [
    "eslint",
    "ts-lint"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "deep-extend": "0.6.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "7.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.16.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-sonarjs": "0.5.0",
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "@rockpack/tester": "0.9.9"
  }
}
