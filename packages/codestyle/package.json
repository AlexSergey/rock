{
  "name": "@rockpack/codestyle",
  "version": "2.0.0-rc.5",
  "description": "The best practice for ESLint and TSLint",
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint \"**\"",
    "test": "node tests.js",
    "test:watch": "node tests.js --watch",
    "production": "npm run lint && npm test && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexSergey/rockpack.git"
  },
  "author": "Aleksandrov Sergey <gooddev.sergey@gmail.com> (https://github.com/AlexSergey/rockpack)",
  "license": "MIT",
  "homepage": "https://www.rockpack.io/",
  "bugs": {
    "url": "https://github.com/AlexSergey/rockpack/issues"
  },
  "keywords": [
    "eslint",
    "ts-lint"
  ],
  "private": false,
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-eslint": "10.1.0",
    "deep-extend": "0.6.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@rockpack/tester": "2.0.0-rc.5"
  }
}
