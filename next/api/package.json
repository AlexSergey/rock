{
  "name": "node-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts.build",
    "build": "cross-env NODE_ENV=production node scripts.build",
    "start:prod": "node dist/index.js",
    "typing": "cross-env NODE_ENV=production tsc -p . --noEmit",
    "lint": "cross-env NODE_ENV=production eslint \"src/**\"",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env test",
    "migrate:production": "sequelize db:migrate --env production",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env test",
    "seed:production": "sequelize db:seed:all --env production",
    "test": "node scripts.tests.js",
    "test:watch": "node scripts.tests.js --watch",
    "fixtures": "ts-node scripts.fixtures.ts",
    "fixtures:test": "ts-node scripts.fixtures.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "3.0.0",
    "@koa/multer": "2.0.2",
    "@koa/router": "8.0.8",
    "bcrypt": "4.0.1",
    "debug": "4.1.1",
    "dotenv-safe": "8.2.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.11.0",
    "koa-bodyparser": "4.2.1",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "multer": "1.4.2",
    "mysql2": "2.1.0",
    "pino": "6.2.1",
    "pino-pretty": "4.0.0",
    "pretty-error": "2.1.1",
    "reflect-metadata": "0.1.13",
    "sanitize-html": "1.23.0",
    "sequelize": "5.21.8",
    "sequelize-cli": "5.5.1",
    "sharp": "0.25.3",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@rockpack/codestyle": "0.9.0",
    "@rockpack/compiler": "0.9.0",
    "@rockpack/tester": "0.9.0",
    "@types/dotenv-safe": "8.1.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/koa": "2.11.3",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-mount": "4.0.0",
    "@types/koa-router": "7.4.0",
    "@types/koa-static": "4.0.1",
    "@types/node": "14.0.4",
    "@types/validator": "13.0.0",
    "sequelize-fixtures": "1.1.1",
    "@types/sequelize-fixtures": "0.6.2"
  }
}
